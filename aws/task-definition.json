{
    "containerDefinitions": [
        {
            "essential": true,
            "image": "rabbitmq",
            "name": "rabbitmq",
            "portMappings": [
                {
                    "containerPort": 5672,
                    "hostPort": 5672
                }
            ],
            "environment": [
                {
                    "name": "RABBITMQ_SERVER_ADDITIONAL_ERL_ARGS",
                    "value": "-setcookie trendrrabbitmqcookie"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "ecs-trendr-service-logs",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "rabbitmq"
                }
            },
            "healthCheck": {
                "command" : [ "CMD-SHELL", "rabbitmq-diagnostics -q ping" ],
                "interval" : 10,
                "retries" : 6,
                "startPeriod" : 60,
                "timeout" : 5
            }
        }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "inferenceAccelerators": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::514592108082:role/ecs-trendr-execution-role",
    "family": "ecs-trendr-task-definition",
    "taskRoleArn": "",
    "placementConstraints": [],
    "volumes": [
        {
            "name": "db-volume"
        }
    ]
}
