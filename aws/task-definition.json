{
    "containerDefinitions": [
        {
            "essential": true,
            "image": "react_frontend",
            "name": "client",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "ecs-trendr-service-logs",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "client"
                }
            }
        }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "inferenceAccelerators": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::514592108082:role/ecs-trendr-execution-role",
    "family": "ecs-trendr-task-definition",
    "taskRoleArn": "",
    "placementConstraints": [],
    "volumes": [
        {
            "name": "db-volume"
        }
    ]
}
