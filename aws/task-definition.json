{
    "containerDefinitions": [
        {
            "command": [
                "/start"
            ],
            "environment": [
                {
                    "name": "FLASK_APP",
                    "value": "trendr"
                }
            ],
            "essential": true,
            "image": "flask_backend",
            "mountPoints": [
                {
                    "containerPath": "/app",
                    "sourceVolume": "_"
                }
            ],
            "name": "api",
            "portMappings": [
                {
                    "containerPort": 5000,
                    "hostPort": 5000
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "ecs-trendr-service-logs-groups",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "api"
                }
            }
        },
        {
            "command": [
                "/start-celerybeat"
            ],
            "environment": [
                {
                    "name": "FLASK_APP",
                    "value": "trendr"
                }
            ],
            "essential": true,
            "image": "flask_celery_beat",
            "mountPoints": [
                {
                    "containerPath": "/app",
                    "sourceVolume": "_"
                }
            ],
            "name": "celery_beat",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "ecs-trendr-service-logs-groups",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "celery_beat"
                }
            }
        },
        {
            "command": [
                "/start-celeryworker-general"
            ],
            "environment": [
                {
                    "name": "FLASK_APP",
                    "value": "trendr"
                }
            ],
            "essential": true,
            "image": "flask_celery_worker_general",
            "mountPoints": [
                {
                    "containerPath": "/app",
                    "sourceVolume": "_"
                }
            ],
            "name": "celery_worker_general",
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "ecs-trendr-service-logs-groups",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "celery_worker_general"
                }
            }
        },
        {
            "essential": true,
            "image": "react_frontend",
            "mountPoints": [
                {
                    "containerPath": "/usr/src/frontend/node_modules",
                    "sourceVolume": "UsrSrcFrontendNode_Modules"
                },
                {
                    "containerPath": "/usr/src/frontend",
                    "sourceVolume": "_React-Frontend"
                }
            ],
            "name": "client",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 3000
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "ecs-trendr-service-logs-groups",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "client"
                }
            }
        },
        {
            "environment": [
                {
                    "name": "FLASK_APP",
                    "value": "trendr"
                }
            ],
            "essential": true,
            "image": "mher/flower",
            "name": "flower",
            "portMappings": [
                {
                    "containerPort": 5555,
                    "hostPort": 5555
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "ecs-trendr-service-logs-groups",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "flower"
                }
            }
        },
        {
            "essential": true,
            "image": "rabbitmq",
            "name": "rabbitmq",
            "portMappings": [
                {
                    "containerPort": 5672,
                    "hostPort": 5672
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "ecs-trendr-service-logs-groups",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "rabbitmq"
                }
            }
        }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "inferenceAccelerators": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::514592108082:role/ecs-trendr-execution-role",
    "family": "ecs-devops-task-definition",
    "taskRoleArn": "",
    "placementConstraints": [],
    "volumes": [
        {
            "host": {
                "sourcePath": "/usr/src/frontend/node_modules"
            },
            "name": "UsrSrcFrontendNode_Modules"
        },
        {
            "host": {
                "sourcePath": "./react-frontend"
            },
            "name": "_React-Frontend"
        },
        {
            "host": {
                "sourcePath": "."
            },
            "name": "_"
        }
    ]
}
