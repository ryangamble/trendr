{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": "off",
        "no-loss-of-precision": "off",
        "react/jsx-key": "off",
        "semi": 0,
        "quotes": 0
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
